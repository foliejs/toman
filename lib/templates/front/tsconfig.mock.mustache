{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "./mock",
    "target": "ES5",
    "typeRoots": [
      "node_modules/@types"
    ],

    "importHelpers": false,
    "experimentalDecorators": true,
    "newLine": "lf",
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "mock"
  ]
}
